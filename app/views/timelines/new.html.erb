<div class="text-center">
<h2>New Timeline</h2>

  <%= simple_form_for(@timeline) do |f| %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.association :events, input_html: { data: { controller: 'select2' } } %>
    <%= f.input :photo, as: :file %>
    <%= f.submit class: "submit-btn"%>
  <% end %>

  <div class="mt-3">
    <%= link_to "Dashboard", dashboard_path, class: "submit-btn" %>
  </div>
</div>
</div>
